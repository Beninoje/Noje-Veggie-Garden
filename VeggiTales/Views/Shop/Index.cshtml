@model IEnumerable<Category>

@{
    ViewData["Title"] = "Shop our Veggies";
}

<h1>Choose a Category</h1>
<ul>
    @foreach (var category in Model)
    {
        <li>
            <a asp-controller="Shop" asp-action="ByCategory" asp-route-name="@category.Name">@category.Name</a>
        </li>
    }

</ul>
